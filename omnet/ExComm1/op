all: teste.o
	g++ -o mysterio teste.o mysterio.o

teste.o:	teste.cc teste.h
	g++ -c teste.cc
mysterio.o:	mysterio.cc teste.h
	g++ -c mysterio.cc
clean:
	rm -rf *.o

vai:
	./mysterio

